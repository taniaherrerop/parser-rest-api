{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "max-line-length": [
      true,
      120
    ],
    "prefer-template": false,
    "interface-name": [true, "never-prefix"],
    "import-name": false,
    "no-console": false
  },
  "rulesDirectory": []
}
